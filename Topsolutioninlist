def _sort(scores, numbers):
    swapped = True

    while swapped:
        swapped = False
        for i in range(0, len(scores)-1):
            if scores [i] < scores [i+1]:
                temp = scores[i]
                scores[i]=scores[i+1]
                scores[i+1] = temp
                temp = numbers[i]
                numbers[i] = numbers[i+1]
                numbers[i+1] = temp
                swapped = True
scores = [63000,63000,126000,126000,12600,25200,6300,6300,18900,3150,12600,37800,12600,1890,1890,1260,12600,31500,12600,1890,1890,1260,3150,4410,2520,31500,31500,12600,31500,31500,12600,6300,6300,6300,6300,6300,6300,15750,25200,12600,12600,18900,9450,7560,8820,6300,12600,18900,12600,12600,18900,12600,126000,126000,9450,9450,9450,12600,9450,9450,12600,12600,12600,12600,12600,12600,1890,1890,1890,63000,63000,31500,63000,63000,126000,126000,31500,126000,126000,37800,75600,189000,226800,18900,12600,12600,12600,12600,12600,12600,12600,6300,6300,6300,6300,6300,12600,12600,12600,12600,6300,6300,6300,12600,18900,6300,6300,6300,6300,12600,12600,6300,6300,6300,6300,6300,12600,12600,6300,6300,6300,6300,3780,6300,5040,7560,12600,10080,5040,5040,5040,22680,22680,22680,15120,15120,15120,15120,15120,15120,22680,22680,22680,5040,5040,5040,5040,5040,5040,15120,15120,15120,5040,5040,5040,5040,5040,5040,15120,15120,15120,5040,5040,5040,7560,12600,6300,12600,25200,12600,12600,18900,6300,7560,12600,6300,7560,12600,6300,18900,28350,9450,2520,3150,3150,4410,6300,3150]

number_of_scores = len(scores)
solution_numbers = list(range(number_of_scores))

_sort(scores, solution_numbers)

print('Top Solutions')
for i in range (0,5):
    print(str(i+1)+ ')',
          'Solution #' + str(solution_numbers[i]),
          'score:', scores[i])
