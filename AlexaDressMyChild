weather = int(input("What is the temperature outside?:"))
print (weather)

if weather >= 72:
    print ("Shorts and Short Sleeves")
elif weather <72 and weather >= 62:
    print ("Long pants and short sleeves")
elif weather <62 and weather >=40:
    print ("Long pants and long sleeves")
elif weather <40 and weather >= -100:
    print ("Cozy Long pants and long sleeves")
